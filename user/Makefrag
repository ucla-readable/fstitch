OBJDIRS += user
BIN += $(OBJDIR)/user/fsync $(OBJDIR)/user/testopgroup

$(OBJDIR)/user/%: $(OBJDIR)/user/%.o
	@echo + ld $@
	$(V)$(CC) $(CFLAGS) -o $@ $@.o -L$(OBJDIR)/lib

$(OBJDIR)/user/testopgroup: $(OBJDIR)/user/testopgroup.o $(OBJDIR)/lib/libopgroup.so
	@echo + ld $@
	$(V)$(CC) $(CFLAGS) -o $@ $@.o -L$(OBJDIR)/lib -lopgroup
